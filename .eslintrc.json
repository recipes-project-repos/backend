{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
      "standard-with-typescript",
      "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "indent": ["error", 2],
      "linebreak-style": ["error", "windows"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "curly": ["error", "multi", "consistent"],
      "default-case": "error",
      "eqeqeq": ["error", "always"],
      "no-else-return": "error",
      "no-console": "error"
    }
}
